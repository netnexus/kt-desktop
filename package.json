{
  "name": "kt-desktop",
  "productName": "Kt Desktop",
  "version": "0.9.33",
  "homepage": "https://netnexus.de",
  "description": "Kt Desktop",
  "author": {
    "name": "NetNexus IT Services",
    "email": "info@netnexus.de"
  },
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "npm run postinstall:electron && npx electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "ng": "ng",
    "publish": "npm version patch && npm run build:prod && export $(cat .env | xargs) && npx electron-builder build --linux --windows --mac --publish always",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "build": "npm run postinstall:electron && npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "export $(cat .env | xargs) && npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "export $(cat .env | xargs) && npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "export $(cat .env | xargs) && npm run build:prod && npx electron-builder build --mac",
    "test": "npm run postinstall:web && ng test",
    "e2e": "npm run postinstall:web && ng e2e"
  },
  "dependencies": {
    "@netnexus/ikontist": "^0.4.10",
    "angular2-csv": "0.2.5",
    "electron-log": "^2.2.17",
    "electron-updater": "^4.0.6",
    "electron-window-state": "^5.0.3",
    "iban": "0.0.12",
    "keytar": "^4.9.0",
    "ng-auto-complete": "^2.10.5",
    "ng2-currency-mask": "^5.3.1",
    "ng2-search-filter": "^0.4.7",
    "ngx-avatar": "^2.9.0",
    "rxjs-compat": "^6.5.2",
    "uikit": "^3.1.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.5",
    "@angular/cli": "^7.3.4",
    "@angular/common": "^7.2.7",
    "@angular/compiler": "^7.2.7",
    "@angular/compiler-cli": "^7.2.7",
    "@angular/core": "^7.2.7",
    "@angular/forms": "^7.2.7",
    "@angular/http": "^7.2.15",
    "@angular/language-service": "^7.2.7",
    "@angular/platform-browser": "^7.2.7",
    "@angular/platform-browser-dynamic": "^7.2.7",
    "@angular/router": "^7.2.7",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "8.9.4",
    "codelyzer": "^4.5.0",
    "core-js": "^2.6.5",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "electron": "^3.0.0",
    "electron-builder": "^20.43.0",
    "electron-publisher-s3": "^20.17.2",
    "electron-reload": "^1.4.0",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "^1.4.2",
    "npm-run-all": "^4.1.5",
    "npx": "10.2.0",
    "protractor": "^5.4.2",
    "rxjs": "^6.5.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.17.0",
    "typescript": "^3.1.1",
    "wait-on": "2.1.0",
    "webdriver-manager": "^12.1.5",
    "zone.js": "^0.8.26"
  }
}
